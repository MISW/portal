{
  "name": "mis-portal-frontend",
  "version": "2.0.0",
  "license": "MPL-2.0",
  "scripts": {
    "coverage": "cp -v ./.next/swc-trace-profile-*.json ./coverage",
    "build": "next build",
    "build:css": "tailwindcss -o src/tailwind.css --minify",
    "dev": "next dev",
    "dev:css": "tailwindcss -o src/tailwind.css --watch",
    "docker": "next dev --hostname 0.0.0.0",
    "export": "next export",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "next lint -d src",
    "lint:fix": "next lint --fix -d src",
    "start": "next start --hostname 0.0.0.0",
    "turbo": "next dev --turbo",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@fortawesome/fontawesome-common-types": "6.6.0",
    "@fortawesome/free-brands-svg-icons": "6.6.0",
    "@mui/icons-material": "5.16.7",
    "@mui/material": "5.16.7",
    "@reduxjs/toolkit": "1.9.7",
    "clsx": "2.1.1",
    "ky": "1.7.2",
    "next": "14.0.4",
    "next-redux-wrapper": "8.1.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "8.1.3",
    "sharp": "0.32.6",
    "styled-components": "6.1.13",
    "tss-react": "4.9.13"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-redux": "7.1.34",
    "@types/sharp": "0.31.1",
    "@types/styled-components": "5.1.34",
    "@types/webpack-env": "1.18.5",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.1",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "9.1.0",
    "postcss": "8.4.49",
    "prettier": "3.3.2",
    "tailwindcss": "3.4.15",
    "type-fest": "4.27.0",
    "typescript": "5.5.3"
  }
}
