{
  "name": "mis-portal-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "build:css": "tailwindcss -o src/tailwind.css --minify",
    "dev": "next",
    "dev:css": "tailwindcss -o src/tailwind.css --watch",
    "docker": "next --hostname 0.0.0.0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "next lint --dir src --ext .js,jsx,.ts,.tsx",
    "lint:fix": "next lint --fix --dir src --ext .js,jsx,.ts,.tsx",
    "start": "next start --hostname 0.0.0.0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fortawesome/fontawesome-common-types": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@mui/icons-material": "^5.6.0",
    "@mui/material": "^5.6.0",
    "@mui/styles": "^5.6.0",
    "@reduxjs/toolkit": "^1.8.1",
    "clsx": "^1.1.1",
    "focus-visible": "^5.2.0",
    "ky": "^0.30.0",
    "next": "^12.1.4",
    "next-redux-wrapper": "^7.0.5",
    "next-transpile-modules": "^9.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^7.2.8",
    "sharp": "^0.30.3",
    "tailwindcss": "^3.0.23"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.23",
    "@types/sharp": "^0.30.1",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.12",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.6.2",
    "tailwind-variant.macro": "^0.0.2",
    "type-fest": "^2.12.2",
    "typescript": "^4.6.3"
  }
}
