{
  "name": "mis-portal-frontend",
  "version": "2.0.0",
  "license": "MPL-2.0",
  "scripts": {
    "coverage": "cp -v ./.next/swc-trace-profile-*.json ./coverage",
    "build": "next build",
    "build:css": "tailwindcss -o src/tailwind.css --minify",
    "dev": "next dev",
    "dev:css": "tailwindcss -o src/tailwind.css --watch",
    "docker": "next dev --hostname 0.0.0.0",
    "export": "next export",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "next lint -d src",
    "lint:fix": "next lint --fix -d src",
    "start": "next start --hostname 0.0.0.0",
    "turbo": "next dev --turbo",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@fortawesome/fontawesome-common-types": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@mui/icons-material": "5.15.1",
    "@mui/material": "5.15.1",
    "@reduxjs/toolkit": "1.9.7",
    "clsx": "2.0.0",
    "ky": "1.1.3",
    "next": "14.0.4",
    "next-redux-wrapper": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "sharp": "0.32.6",
    "styled-components": "6.1.2",
    "tss-react": "4.9.3"
  },
  "devDependencies": {
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/react-redux": "7.1.33",
    "@types/sharp": "0.31.1",
    "@types/styled-components": "5.1.34",
    "@types/webpack-env": "1.18.4",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "postcss": "8.4.32",
    "prettier": "3.1.1",
    "tailwindcss": "3.4.0",
    "type-fest": "4.8.3",
    "typescript": "5.3.3"
  }
}
