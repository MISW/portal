{
  "name": "mis-portal-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "build:css": "tailwindcss -o src/tailwind.css --minify",
    "dev": "next",
    "dev:css": "tailwindcss -o src/tailwind.css --watch",
    "docker": "next --hostname 0.0.0.0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "next lint --dir src --ext .js,jsx,.ts,.tsx",
    "lint:fix": "next lint --fix --dir src --ext .js,jsx,.ts,.tsx",
    "start": "next start --hostname 0.0.0.0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fortawesome/fontawesome-common-types": "^0.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.4.0",
    "@mui/styles": "^5.3.0",
    "@reduxjs/toolkit": "^1.7.1",
    "clsx": "^1.1.1",
    "focus-visible": "^5.2.0",
    "ky": "^0.28.7",
    "next": "^12.0.10",
    "next-redux-wrapper": "^7.0.5",
    "next-transpile-modules": "^9.0.0",
    "node-fetch": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "sharp": "^0.30.0",
    "tailwindcss": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/sharp": "^0.29.5",
    "@types/tailwindcss": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.3.0",
    "prettier": "^2.5.1",
    "tailwind-variant.macro": "^0.0.2",
    "type-fest": "^2.11.1",
    "typescript": "^4.5.5"
  }
}
