{
  "name": "mis-portal-frontend",
  "scripts": {
    "coverage": "cp -v ./.next/swc-trace-profile-*.json ./coverage",
    "build": "next build",
    "build:css": "tailwindcss -o src/tailwind.css --minify",
    "dev": "next dev",
    "dev:css": "tailwindcss -o src/tailwind.css --watch",
    "docker": "next dev --hostname 0.0.0.0",
    "export": "next export",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "next lint --dir src --ext .js,jsx,.ts,.tsx",
    "lint:fix": "next lint --fix --dir src --ext .js,jsx,.ts,.tsx",
    "start": "next start --hostname 0.0.0.0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fortawesome/fontawesome-common-types": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.6",
    "@mui/styles": "^5.10.6",
    "@reduxjs/toolkit": "^1.8.5",
    "clsx": "^1.2.1",
    "focus-visible": "^5.2.0",
    "ky": "^0.31.3",
    "next": "^12.3.1",
    "next-redux-wrapper": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "sharp": "^0.31.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/sharp": "^0.31.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "autoprefixer": "^10.4.11",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "type-fest": "^2.19.0",
    "typescript": "^4.7.4"
  }
}
