{
  "name": "mis-portal-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "build:css": "tailwindcss -o src/tailwind.css --minify",
    "dev": "next",
    "dev:css": "tailwindcss -o src/tailwind.css --watch",
    "docker": "next --hostname 0.0.0.0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "next lint --dir src --ext .js,jsx,.ts,.tsx",
    "lint:fix": "next lint --fix --dir src --ext .js,jsx,.ts,.tsx",
    "start": "next start --hostname 0.0.0.0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@fortawesome/fontawesome-common-types": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@mui/icons-material": "5.6.2",
    "@mui/material": "5.7.0",
    "@mui/styles": "5.7.0",
    "@reduxjs/toolkit": "1.8.1",
    "clsx": "1.1.1",
    "focus-visible": "5.2.0",
    "ky": "0.30.0",
    "next": "12.1.6",
    "next-redux-wrapper": "7.0.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "8.0.1",
    "sharp": "0.30.4"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@types/node": "17.0.32",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/react-redux": "7.1.24",
    "@types/sharp": "0.30.2",
    "@types/tailwindcss": "3.0.10",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "autoprefixer": "10.4.7",
    "babel-plugin-macros": "3.1.0",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "postcss": "8.4.13",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-focus-visible": "6.0.4",
    "postcss-import": "14.1.0",
    "postcss-nested": "5.0.6",
    "postcss-nesting": "10.1.5",
    "postcss-preset-env": "7.5.0",
    "prettier": "2.6.2",
    "tailwind-variant.macro": "0.0.2",
    "tailwindcss": "3.0.24",
    "type-fest": "2.12.2",
    "typescript": "4.6.4"
  }
}
